3	/galerias/id	GET	galeria
4	/noticias/	GET	noticias
5	/noticias/id	GET	noticia
6	/cursos /	GET	cursos
7	/cursos/id	GET	curso
8	/login	GET	login
9	/cadastro	GET	cadastro
